{
  "name": "bitbank-depth-chart",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "fix": "prettier --write .",
    "test": "jest",
    "upgrade-deps": "yarn upgrade-interactive",
    "sync-deps": "npx syncyarnlock -s -k"
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "bignumber.js": "^9.0.1",
    "highcharts": "^9.3.2",
    "highcharts-react-official": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use-bitbank": "^0.1.8"
  },
  "devDependencies": {
    "@swc/core": "^1.2.118",
    "@swc/jest": "^0.2.11",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/bignumber.js": "^5.0.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.1",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2",
    "vite": "^2.7.0"
  },
  "jest": {
    "transform": {
      ".+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testEnvironment": "jsdom"
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
