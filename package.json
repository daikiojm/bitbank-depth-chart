{
  "name": "bitbank-depth-chart",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "fix": "prettier --write .",
    "test": "jest",
    "upgrade-deps": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.0",
    "bignumber.js": "^9.0.1",
    "highcharts": "^9.3.1",
    "highcharts-react-official": "^3.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-use-bitbank": "^0.1.7"
  },
  "devDependencies": {
    "@swc/core": "^1.2.112",
    "@swc/jest": "^0.2.9",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/bignumber.js": "^5.0.0",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "jest": {
    "transform": {
      ".+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testEnvironment": "jsdom"
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
